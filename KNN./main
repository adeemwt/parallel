#include "sr_header.h"

int main(void)
{

	point* arr = (point*)malloc(sizeof(point)*L);
	generateRandomIntArray(arr, L, M);
	print(arr, L);
	int** res;
	clock_t startT, stop;
	startT = clock();
	res = start(0, L, L, arr);
	stop = clock();
	printf("\Algorithm complete...");
	print_elapsed(startT, stop);	
	getchar();
	free(res);

	free(arr);
}
